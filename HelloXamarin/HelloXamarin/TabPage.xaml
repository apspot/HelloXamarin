<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage
  xmlns:local="clr-namespace:HelloXamarin"
  xmlns="http://xamarin.com/schemas/2014/forms"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  x:Class="HelloXamarin.TabPage">
  <local:ListPage2 Title="Page 1" Icon="clock.png"/>
  <NavigationPage Title="Page 2" Icon="clock.png">
    <x:Arguments>
      <local:NavMasterDetailPage/>
    </x:Arguments>
  </NavigationPage>
  <ContentPage Title="Page 3" Icon="clock.png">
    <Label Text="Content of page 3" HorizontalOptions="Center" VerticalOptions="Center"/>
  </ContentPage>
</TabbedPage>